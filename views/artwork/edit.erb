<div class="sign-up-card">
<div class="sign-up-info">
<h1>Project Details</h1>
<p>Update your project details below</p>
</div>



<form class='sign-up-forms' action="/create/<%= project['id'] %>" method="POST">
<input type="hidden" name="_method" value="PUT"/>
  <input type="text" placeholder="title" name="project_title" value="<%= project['project_title'] %>">
   <input type="text" placeholder="thumbnail_img" name="project_thumnail" value="<%= project['project_thumnail'] %>">
    <input type="text" placeholder="author" name="project_author" value="<%= project['project_author'] %>" >
     <input type="text" placeholder="about" name="project_about" value="<%= project['project_about'] %>">
      <input type="text" placeholder="image_url" name="project_main_img" value="<%= project['project_main_img'] %>">
  <button class="sign-up-button">Update Project</button>
</form>

</div>